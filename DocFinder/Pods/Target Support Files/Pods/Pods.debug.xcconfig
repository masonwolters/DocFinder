GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/TwilioSDK"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/TwilioSDK/Libraries"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/TwilioSDK"
OTHER_LDFLAGS = -ObjC -l"Pods-TwilioSDK" -l"TwilioClient" -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "SystemConfiguration"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods